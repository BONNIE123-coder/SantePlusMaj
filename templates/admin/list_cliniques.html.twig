						<div class="row">
							<div class="col-12"> 
                  <!-- Recent Order Table -->
                 <div class="main-panel">
        <div class="content-wrapper">
          
          <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
              <div class="card">
                <div class="card-body">
                  <h4 class="card-title text-bold text-dark">Liste des Cliniques</h4>
                  <div class="table-responsive">
                    <table class="table table-striped text-dark">
                      <thead>
                        <tr>
                          <th>
                            Id
                          </th>
                          <th>
                            Nom
                          </th>
                          <th>
                            Adresse
                          </th> <th>
                            Region
                          </th>
                          <th>
                            Telephone
                          </th>
                          <th class="text-right">
                            Option
                          </th>
                         
                        </tr>
                      </thead>
                      <tbody>
                        {% for clinique in pagination %}
                        <tr>
                          <td>
                            {{ clinique.id }} 
                          </td>
                          <td>
                            {{ clinique.nom }} 
                          </td>
                          <td>
                           {{ clinique.adresse }}
                          </td>
                          <td>
                           {{ clinique.region }}
                          </td>
                          <td>
                           {{ clinique.telephone }}
                          </td>
                          <td class="text-right">
                              <div class="dropdown show d-inline-block widget-dropdown">
                                <a class="dropdown-toggle icon-burger-mini" href="#" role="button" id="dropdown-recent-order2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false" data-display="static"></a>
                                <ul class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdown-recent-order2">
                                  <li class="dropdown-item">
                                    <a href="{{ path('app_hopitaux_edit', {'id': clinique.id}) }}">Update</a>
                                  </li>
                                  <li class="dropdown-item">
                                    <a href="{{ path('app_hopitaux_delete', {'id': clinique.id}) }}">Delete</a>
                                  </li>
                                </ul>
                              </div>
                            </td>
                        </tr>
                         {% endfor %}
                      </tbody>
                    </table>
                     <div class="col-16" >
                        <div class="row justify-content-center" >
                          <div class="pagination justify-content-center">
                            {{ knp_pagination_render(pagination) }}
                          </div>
                        </div>
                    </div> 
                  </div>
                </div>
              </div>
            </div>
